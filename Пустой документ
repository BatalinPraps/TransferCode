package aTaskHabr.basics;

public class Task2 {

    public static void main (String[] args){

        Method();

    }

    public static void Method() {
        int a = 10;
        int[] array = new int[a];
        for (int i = 0; i < array.length; i++) {
            array[i] = (int) (Math.random() * 100);
            System.out.println(array[i]);
        }
    }
}
